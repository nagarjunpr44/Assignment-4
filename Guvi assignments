var timesout=3;
var Battingaverage=runsscored/timesout;
console.log(Battingaverage);

//8.Calculate five test scores and print their average

var test1=80,test2=95,test3=83,test4=77,test5=97;
var sum=test1+test2+test3+test4+test5;
var average=sum/5;
console.log(average);

//9.Power of any number x ^ y

var x=10;
var y=3;
var power=x^y;
console.log(power);

//10. Calculate Simple Interest

var p=1000,t=2,r=10,simpleinterest;

simpleinterest=(p*t*r)/100;

console.log(simpleinterest);

//11.Calculate area of an equilateral triangle

var a=12,area;
area=(1.73*a*a)/4;
console.log(area);

//12.Area Of Isosceles Triangle

var h=5,b=10,area;
area=(1*b*h)/2;
console.log(area);

//13.Volume Of Sphere

var r=12,Volume;
Volume=(2*3.14*r*r*r)/3;
console.log(Volume);

//14.volume of prism

var l=10,b=12;h=8,volume;
volume=(l*b*h)/2;
console.log(volume);

//15.Find area of a triangle

var b=8,h=9,area;
area=(b*h)/2;
console.log(area);

//16.Give the Actual cost and Sold cost, Calculate Discount Of Product

var actualcost=100,soldcost=95,Discount;
Discount=(actualcost-soldcost);
console.log(Discount);

//17.Given their radius of a circle and find its diameter, circumference and area.

var r=10,diameter,circumference,area;

diameter=2*r;
circumference=2*3.14*r;
area=3.14*r*r;
console.log(diameter);
console.log(circumference);
console.log(area);

