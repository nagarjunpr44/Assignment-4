//2.Swapping 2 numbers

let a = 2;
let b = 3;

a = a + b;
b = a - b;
a = a - b;

console.log(a);
console.log(b);

//3.Addition of 3 numbers
var a,b,c;
var sum=a+b+c;
console.log(sum);

//4.Celsius to Fahrenheit conversion

var Celsius=40;
var Fahrenheit;
Fahrenheit=(Celsius*1.8)+32;
console.log(Fahrenheit);

//5.Meter to miles

var meter=300;
var miles;
miles=meter*0.000621;
console.log(miles);

//6.Pounds to kg

var Pounds=100;
var kg;
kg=Pounds*0.453592;
console.log(kg);

//7.Calculate Batting Average

